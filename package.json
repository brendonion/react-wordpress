{
  "name": "react-wordpress",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build-js": "react-scripts build",
    "build-css": "node-sass-chokidar src/styles/scss -o src/styles/css",
    "watch-css": "node-sass-chokidar src/styles/scss -o src/styles/css --watch --recursive",
    "dev": "npm-run-all -p watch-css start",
    "flow": "flow",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass-chokidar": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "flow-bin": "^0.68.0"
  }
}
